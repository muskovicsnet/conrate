<%= do_edit({
    title: t(".blogs"),
    columns: [
      Btemplater::NewDecorator.new(
        :title,
        Proc.new do |f, c|
          f.input :title, autofocus: true
        end
      ),
      :description
    ],
    item: @obj,
    model: Conrateblogs::Blog,
    url: conrateblogs.blog_path(@obj),
    back_url: @obj,
    method: :post
  })
%>