<%= do_edit({
    title: t(".page"),
    columns: [
      :title,
      Btemplater::NewDecorator.new(
        :content,
        Proc.new do |f, c|
          f.input :content, :as => :ckeditor, :input_html => { :ckeditor => {:toolbar => 'Full'} }
        end
      )
    ],
    item: @obj,
    model: Conratepages::Page,
    url: conratepages.page_path(@obj),
    back_url: @obj,
    method: :post
  })
%>