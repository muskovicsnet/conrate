.my-background
{
    background: linear-gradient(to bottom, #207cca 0%,#efebdd 0%,#f6f4ed 66%,#ebe7d9 100%) !important;
}

.full-width
{
    margin: 0 auto !important;
    width: 100% !important;
}

.my-footer
{
    background-color: #7F7355 !important;
    color: white !important;
}

.searchbar {
  background-color: #9D9073;
  padding: 4px 4px 4px 4px !important;
  border-radius: 4px;
}

.navbar {
  background: linear-gradient(to bottom, #A1CE41 0%, #6AAD38 100%) repeat scroll 0 0 transparent !important;
  background: -moz-linear-gradient(top, #a1ce41 0%, #6aad38 100%) !important;
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#a1ce41), color-stop(100%,#6aad38)) !important;
  background: -webkit-linear-gradient(top, #a1ce41 0%,#6aad38 100%) !important;
  background: -o-linear-gradient(top, #a1ce41 0%,#6aad38 100%) !important;
  background: -ms-linear-gradient(top, #a1ce41 0%,#6aad38 100%) !important;
}
.box {
    border: 1px solid #F4F1E7;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    margin-bottom: 15px;
    padding: 4px;
    overflow: hidden;
}
.box .title_inner_block, #column-right .title_inner_block {
    background: #F3EEDB;
    background: -moz-linear-gradient(top, #F3EEDB 0%, #ffffff 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#F3EEDB), color-stop(100%,#ffffff));
    background: -webkit-linear-gradient(top, #F3EEDB 0%,#ffffff 100%);
    background: -o-linear-gradient(top, #F3EEDB 0%,#ffffff 100%);
    background: -ms-linear-gradient(top, #F3EEDB 0%,#ffffff 100%);
    border-radius: 5px 5px 0 0;
    -webkit-border-radius: 5px 5px 0 0;
    -moz-border-radius: 5px 5px 0 0;
    border-top: 1px solid #F3EEDB;
    color: #FF8106;
    font-weight: normal;
    text-transform: capitalize;
}
.box .box-heading {
    background: url("<%= image_path 'health/cate_icon.png' %>") no-repeat scroll 10px 10px transparent;
    color: #706342;
    padding: 7px 0 7px 30px;
    text-transform: uppercase;
    font: 16px/22px "Roboto",sans-serif;
    text-transform: capitalize;
}
.box .box-content {
    margin: 5px 0 0;
    padding: 5px 5px 0;
}
.box .box-content ul, #content .content ul {
    padding: 0px;
    margin: 0px;
    list-style: none;
    line-height: 24px;
}
.box .box-content ul, #content .content ul {
    padding: 0px;
    margin: 0px;
    list-style: none;
}
.box .box-content ul li, ul.box-category > li {
  line-height: 35px;
}
.box .box-content ul li a, ul.box-category > li a, .box a.list-group-item {
  color: #7F7355;
  padding-top: 4px;
  padding-bottom: 4px;
}
.box .box-content ul li a:hover, ul.box-category > li a.active, .box a.list-group-item:hover, .account-account #content ul.list-unstyled li a:hover, .affiliate-account #content ul.list-unstyled li a:hover {
    background: url("<%= image_path 'health/cat_arrow_hover.png' %>") no-repeat scroll left center transparent;
    color: #FF8A00 !important;
}
.box .box-content ul li a, #content .content ul li a, .box a.list-group-item, .account-account #content ul.list-unstyled li a, .affiliate-account #content ul.list-unstyled li a {
    background: url("<%= image_path 'health/cat_arrow_hover.png' %>") no-repeat scroll left center transparent;
    padding-left: 12px;
}
